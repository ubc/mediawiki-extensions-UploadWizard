{
	"root": true,
	"extends": [
		"wikimedia/client",
		"wikimedia/jquery",
		"wikimedia/mediawiki"
	],
	"globals": {
		"wikibase": "readonly"
	},
	"rules": {
		"max-len": "off",
		"mediawiki/class-doc": "warn",
		"mediawiki/msg-doc": "warn",
		"jquery/no-each-util": "off",
		"jquery/no-map-util": "off",
		"jsdoc/no-undefined-types": [
			"warn",
			{
				"definedTypes": [
					"uw"
				]
			}
		],
		"no-shadow": "warn"
	}
}
